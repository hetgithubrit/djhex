<%- include('../partials/head', { title: 'Services', navLinks, currentPath }) %>

  <div class="wrapper">
    <section class="section" id="services">
      <div class="section-head">
        <div>
          <div class="pill">Services</div>
          <h2>Tailored sound for every moment</h2>
        </div>
      </div>
      <div class="services-grid">
        <% services.forEach(service => { %>
          <div class="service">
            <div class="image">Service Image Placeholder</div>
            <div class="body">
              <h4><%= service.title %></h4>
              <p><%= service.desc %></p>
            </div>
          </div>
        <% }) %>
      </div>
    </section>
  </div>

<%- include('../partials/footer', { year }) %>

